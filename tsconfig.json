{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "./public",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"]
        }
    }
}
